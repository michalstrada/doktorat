@article{Zhou99,
	Abstract = {This paper is concerned with a Petri net model having fuzzy timing and fuzzy real-time temporal logic. In an earlier paper a Petri net model with four fuzzy set theoretic functions was introduced but no algorithms were given for computing the two important operators called earliest and latest. This paper first presents algorithms for performing these two operations on fuzzy functions of time, and then extends the existing concept of (real-time) metric temporal logic to fuzzy metric temporal logic by expressing the metrics for state and time constraints in terms of fuzzy possibility distributions. Finally, the two concepts of into fuzzy timing Petri nets and fuzzy metric temporal logic are combined to introduce a new Petri net model called the temporal-logic fuzzy timing Petri net (TLFTN). The paper concludes with an algorithm and illustrative example for model checking using TLFTNs.},
	Author = {Zhou, Y. and Murata, T.},
	Journal = {International Journal of Intelligent Systems},
	Keywords = {fuzzy-metric temporal logic, fuzzy-timing Petri nets, model checking},
	Number = {8},
